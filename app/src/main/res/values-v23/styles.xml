<resources>
    <style name="ToolbarStyle_InLayout">
        <item name="android:id">@id/toolbar</item>
        <item name="android:background">@color/empty_car_order_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>
    <style name="ToolbarStyle_InLayout.numal">
        <item name="android:id">@id/toolbar</item>
        <item name="android:background">@color/empty_car_order_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:subtitleTextColor">@color/black</item>
        <item name="android:titleTextColor">@color/black</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>
</resources>
